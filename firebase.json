{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "auth": {
      "port": 9099
    },
    "dataconnect": {
      "port": 5432,
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    },
    "ui": {
      "enabled": true,
      "port": 4000
    },
    "hosting": { "port": 5000 }
  },
  "dataconnect": {
    "source": "dataconnect"
  }
}
